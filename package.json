{
  "name": "cro-components",
  "version": "1.0.1",
  "main": "storybook-static/index.html",
  "types": "./cro-component-exports/index.d.ts",
  "bin": {
    "cro-generate": "./generate-component.js",
    "cro-build": "./build-components.js"
  },
  "scripts": {
    "test": "jest",
    "prettier:check": "prettier . --check '!./storybook-static' '!./dist' '!./coverage'",
    "prettier:fix": "npm run prettier:check -- --write",
    "lint:check": "eslint . --max-warnings=0 --ignore-pattern 'storybook-static/*' --ignore-pattern 'coverage/*'",
    "lint:fix": "npm run lint:check -- --fix",
    "source:check": "npm run lint:check && npm run prettier:check",
    "source:fix": "npm run lint:fix && npm run prettier:fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build": "storybook build && rollup -c && npm run generate-exports",
    "generate-exports": "node generate-exports.js",
    "generate-component": "node generate-component.js",
    "postinstall": "node postinstall.js"
  },
  "files": [
    "generate-component.js",
    "build-components.js",
    "postinstall.js",
    "rollup.config.js",
    "generate-exports.js",
    "README.md",
    "stories/"
  ],
  "type": "module",
  "keywords": [
    "web-components",
    "cro",
    "conversion-optimization",
    "ui-components",
    "storybook"
  ],
  "author": "Virgin Media O2",
  "license": "ISC",
  "description": "A collection of reusable web components for Virgin Media O2 CRO tests, with extensible architecture for project-specific components",
  "devDependencies": {
    "@babel/preset-env": "^7.26.9",
    "@chromatic-com/storybook": "^3.2.2",
    "@eslint/eslintrc": "^3.3.0",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/gitlab": "^13.2.4",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@storybook/addon-essentials": "^8.4.5",
    "@storybook/addon-interactions": "^8.6.4",
    "@storybook/addon-jest": "^8.6.7",
    "@storybook/addon-storysource": "^8.4.7",
    "@storybook/addon-styling-webpack": "^1.0.1",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.5",
    "@storybook/blocks": "^8.4.5",
    "@storybook/builder-webpack5": "^8.4.7",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/test": "^8.6.4",
    "@storybook/test-runner": "^0.22.0",
    "@storybook/web-components": "^8.4.7",
    "@storybook/web-components-webpack5": "^8.4.5",
    "@testing-library/jest-dom": "^6.6.3",
    "babel-jest": "^29.7.0",
    "eslint": "^9.22.0",
    "jest-environment-jsdom": "^29.7.0",
    "lit": "^3.2.1",
    "prettier": "^3.5.3",
    "rollup": "^2.79.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^24.2.3",
    "storybook": "^8.4.5",
    "wait-on": "^8.0.3"
  },
  "dependencies": {
    "@rollup/plugin-node-resolve": "^16.0.0"
  },
  "exports": {
    "./Button": "./cro-component-exports/Button.js",
    "./Header": "./cro-component-exports/Header.js",
    "./Overlay": "./cro-component-exports/Overlay.js",
    "./package.json": "./package.json"
  },
  "peerDependencies": {
    "rollup": "^2.79.2",
    "storybook": "^8.4.5"
  }
}
